{
  "manifest_version": 2,
  "name": "Barbra",
  "description": "An awesome extension",
  "version": "0.1.0",
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "browser_action":{
    "default_icon": {
      "19": "icon/icon19.png",
      "38": "icon/icon38.png"
    }
  },
  "permissions":[
    "background",
    "activeTab",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "externally_connectable": {
    "matches": [
      "https://b6bce9e2.ngrok.io/*"
    ]
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["js/inject.js"]
    }
  ],

  "web_accessible_resources": [
    "*"
  ]

}